import pandas as p
import numpy as n


def main():
    
    gradebook_dict = {}
    values = int(input('How many tests are you storing? '))

    keys = int(input('How many students are you storing? '))
    
    for k in range(keys):
        gradebook_dict[k] = input('Please enter a name: ')
        for j in range(values):
            gradebook_dict[j] = input('Please enter the grade: ')
    print(gradebook_dict)
      


main()
